{
    "env": {
        "browser": false,
        "node": true,
        "es2021": true
    },
    "extends": [ "airbnb-base", "prettier", "plugin:node/recommended" ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "error",
        "no-console": "off"
    }
}
